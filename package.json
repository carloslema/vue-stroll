{
  "name": "vue-stroll",
  "version": "0.5.0",
  "description": "Vue.js + Stroll.js",
  "main": "dist/vue-stroll.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --port 8090",
    "demo": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.config.prod.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoluoboding/vue-stroll.git"
  },
  "keywords": [
