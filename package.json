    "stroll",
    "scroll",
    "troll"
  ],
  "author": "xiaoluoboding <xiaoluoboding@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiaoluoboding/vue-stroll/issues"
  },
  "homepage": "https://github.com/xiaoluoboding/vue-stroll",
  "dependencies": {
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "unminified-webpack-plugin": "^1.2.0",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
